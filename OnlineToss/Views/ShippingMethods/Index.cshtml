@model IEnumerable<OnlineToss.Models.ShippingMethod>

@{
    ViewBag.Title = "Index";
}


<h3 class="mt-3">TOSS運送種類</h3>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ShipName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fee)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ShipName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fee)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ShipID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ShipID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ShipID })
        </td>
    </tr>
}

</table>
